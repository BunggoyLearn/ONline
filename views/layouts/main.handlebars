<html lang="en">

  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>ONline</title>
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
  </head>

  <body>
    <header>
      <h1 id="brand-header">ONline</h1>
    </header>
    <nav>
      <div id="home-btn"><a href="/"><b>Home</b></a></div>
      <div id="dashboard-btn"><a href="/dashboard"><b>Dashboard</b></a></div>
      {{#if loggedIn}}
        <div id="logout-btn"><a id="logout" class="logout"><b
            >Logout</b></a></div>
      {{else}}
        <div id="login-btn"><a href="/login"><b>Login</b></a></div>
      {{/if}}
    </nav>
    <main>
      <div class="body-sect">
        {{{body}}}
      </div>
    </main>

    {{#if loggedIn}}
      <script src="/js/logout.js"></script>
    {{/if}}
    <script src="/js/script.js"></script>
  </body>

</html>